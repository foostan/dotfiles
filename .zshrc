# oh-my-zsh configurations
ZSH=$HOME/.oh-my-zsh
ZSH_THEME="my"
COMPLETION_WAITING_DOTS="true"
plugins=(autojump brew bundler copydir copyfile cp gem git git-extras git-flow git-flow-avh git-hubflow git-remote-branch gitfast github history knife knife_ssh osx perl rails rake rsync ruby tmux tmuxinator vagrant)
source $ZSH/oh-my-zsh.sh
